# -----------------------------------------------------
# Identify the full discs on each node.
#[user@trop03]

    source "${HOME}/ssh-options"

    for vmname in ${kfnames[@]}
        do
            echo "---- ----"
            ssh \
                ${sshopts[*]} \
                ${sshuser:?}@${vmname:?} \
                    "
                    hostname
                    date
                    df -h | grep '100%' | awk '{ print \$6}'
                    "
        done

