# Check the Kafka settings on each node
#[user@trop03]

    source "${HOME}/ssh-options"

    for vmname in ${kfnames[@]}
        do
            echo "---- ----"
            ssh \
                ${sshopts[*]} \
                ${sshuser:?}@${vmname:?} \
                    '
                    hostname
                    date
                    cat ${HOME}/kafka.env
                    '
        done

