# -----------------------------------------------------
# Our Kafka nodes.
#[user@desktop]

    kfnames=(
        Stedigo
        Angece
        Edwalafia
        Onoza
        )

#---------------------------------------------------------------------
# Update the ssh keys for each node.
#[user@desktop]

    source "${HOME}/libvirt.settings"
    source "${HOME}/ssh-options"

    for vmname in ${kfnames[@]}
        do
            ssh-keygen \
                -q -R \
                    "${vmname:?}"
        done

    >   /home/Zarquan/.ssh/known_hosts updated.
